{
  "name": "asylum-connect-catalog",
  "version": "0.1.0",
  "description": "Asylum Connect Catalog App powered by the One Degree API",
  "engines": {
    "node": "6.2.2"
  },
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "web-prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js",
    "web-dev-c9": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base public/ --config webpack.config.js --port $PORT --host $IP --inline --hot --colors",
    "web-dev": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base public/ --config webpack.config.js --port 8080 --inline --hot --colors",
    "postinstall": "webpack -p"
  },
  "dependencies": {
    "ajv": "^5.2.2",
    "autosuggest-highlight": "^3.1.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "ejs": "2.5.6",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "fetch-jsonp": "^1.1.3",
    "file-loader": "^1.1.5",
    "json-loader": "^0.5.7",
    "jss": "^8.1.0",
    "langs": "^2.0.0",
    "material-ui": "^1.0.0-beta.10",
    "lodash": "^4.17.4",
    "material-ui-icons": "^1.0.0-beta.17",
    "node-fetch": "^1.7.3",
    "node-noop": "^1.0.0",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "path-to-regexp": "^2.0.0",
    "react": "^15.6.1",
    "react-dimensions": "^1.3.0",
    "react-autosuggest": "^9.3.2",
    "react-dom": "^15.6.1",
    "react-fontawesome": "^1.6.1",
    "react-google-maps": "^9.0.1",
    "react-places-autocomplete": "^5.4.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6"
  },
  "devDependencies": {
    "cross-env": "^5.0.5",
    "request": "^2.81.0",
    "tape": "^4.7.0",
    "webpack-dev-server": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeremyhatter/asylum-connect-1deg-catalog"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
